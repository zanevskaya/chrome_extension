{
	"manifest_version": 2,

	"name": "Hourly Physical",
	"description": "Extension for your health",
	"version": "1.0",
	"icons": {
	"128": "128.png"
		},
	"content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
        "browser_action": {
        "default_title": "HourlyPhysical",
        "default_icon": "128.png",
        "default_popup": "popup.html"
    },
  "background": {
    "scripts": ["background.js"]
  }
}
